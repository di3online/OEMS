----------------------------------------------------------
//Student Module
----------------------------------------------------------
19.
LSTES[CL]		//List all exam information of one student
Cookie: value[CL]
[CL]

Return:
200 OK[CL]
[CL]
<?xml version="1.0"?>
<LSTES>
<exam>			//The number of exams is equal to or above zero
<eid></eid>
<name></name>
<stime></stime>
<etime></etime>
<enablestate></enablestate>
</exam>
</LSTES>

400 NOPERMISSION[CL]
[CL]

20.
LSTERS[CL]		//List all the exam results of one student
Cookie: value[CL]
[CL]

Return:
200 OK[CL]
[CL]
<?xml version="1.0"?>
<LSTERS>
<exam>			//The number of exams is equal to or above zero
<pid></pid>		//Paper id
<name></name>		//Exam name
<score></score>
</exam>
</LSTERS>

400 NOPERMISSION[CL]
[CL]

21.
SEDT pid[CL]		//List one exam result of one student in detail
Cookie: value[CL]
[CL]

Return:
200 OK[CL]
[CL]
<?xml version="1.0"?>
<SEDT>
<question>			//The number of questions is equal to or above zero
<qid></qid>
<cAnswer></cAnswer>
<stuAnswer></stuAnswer>
</question>
</SEDT>

400 NOPERMISSION[CL]
[CL]

404 NOTFOUND[CL]
[CL]

22.
STARTE eid[CL]		//Start an exam
Cookie: value[CL]
[CL]

Return:
200 OK[CL]

400 NOPERMISSION[CL]
[CL]

404 NOTFOUND[CL]
[CL]

23.
GETQNUM eid[CL]		//Get the number of questions in one exam. Database does not store this information.
Cookie: value[CL]
[CL]

Return:
200 OK[CL]
[CL]
<?xml version="1.0"?>
<GETQNUM>
<qnum></qnum>
</GETQNUM>

400 NOPERMISSION[CL]
[CL]

404 NOTFOUND[CL]
[CL]

24.
NEXTQ[CL]			//Get next question
Cookie: value[CL]
[CL]

Return:
200 OK[CL]
[CL]
<?xml version="1.0"?>
<NEXTQ>			
<qid></qid>
<qdescription></qdescription>
<choice>			//The number of choices is either 2 or 4
<cid></cid>
<cdescription></cdescription>
</choice>
</NEXTQ>

400 NOPERMISSION[CL]
[CL]

404 NOTFOUND[CL]
[CL]

25.
UPANS qid cid[CL]		//Upload answer.
Cookie: value[CL]
[CL]

Return:
200 OK[CL]

400 NOPERMISSION[CL]
[CL]

404 NOTFOUND[CL]
[CL]
