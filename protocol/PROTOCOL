OEMS Protocol version 2.2
[CL]="\r\n"
/*
 *Modified EINF(Require adjustment to database)
 *Add MPSTA(Modify paper status to "Y")
 *Add student module instructions
 *Add some necessary comments
 */

OEMS Protocol version 2.3
/*
 *Modify LSTQ(add tag description)
 */

OEMS Protocol version 2.4
/*
 *Modify USRADD
 */

OEMS Protocol version 2.5
/*
 *Modify EINF
 */
--------------------------------------------------------------------------
//User Module
--------------------------------------------------------------------------
1.
LOGIN uid pwd[CL]
[CL]

Return:
200 OK[CL]
[CL]
<?xml version="1.0"?>
<LOGIN>
<gid></gid>
<cookie></cookie>
</LOGIN>

403 MISMATCH[CL]
[CL]

2.
USRINF uid[CL]
Cookie: value[CL]
[CL]

Return:
200 OK[CL]
[CL]
<?xml version="1.0"?>
<USRINF>
<uid></uid>
<name></name>
<email></email>
<phone></phone>
</USRINF>

404 NOTFOUND[CL]
[CL]

3.
MUSRINF uid[CL]
Cookie: value[CL]
[CL]
<?xml version="1.0"?>
<MUSRINF>
<email></email>
<phone></phone>
</MUSRINF>

Return:
200 OK[CL]
[CL]

400 NOPERMISSION[CL]
[CL]

404 NOTFOUND[CL]
[CL]

4.
PWDCHG uid[CL]
Cookie: value[CL]
[CL]
<?xml version="1.0"?>
<PWDCHG>
<old></old>
<new></new>
</PWDCHG>

Return:
200 OK[CL]
[CL]

400 NOPERMISSION[CL]
[CL]

404 NOTFOUND[CL]
[CL]

--------------------------------------------------------------------
//Teacher module
--------------------------------------------------------------------
5.
LSTET[CL]
Cookie: value[CL]
[CL]

Return:
200 OK[CL]
[CL]
<?xml version="1.0"?>
<LSTET>
<exam>			//The number of exams is equal to or above zero
<eid></eid>
<name></name>
<course></course>
<stime></stime>
<etime></etime>
<accomplishment>value</accomplishment>	//If all three papers is finished, value="YES", Else value="NO"
<management>value</management>		//Value=online or offline
<status>value</status>			//Value=closed, started or finished
</exam>
</LSTET>

400 NOPERMISSION[CL]
[CL]

6.
EINF eid[CL]
Cookie: value[CL]
[CL]

Return:
200 OK[CL]
[CL]
<EINF>
<name></name>
<course></course>
<stime></stime>
<etime></etime>
<accomplishment>value</accomplishment>	//If all three papers is finished, value="YES", Else value="NO"
<management>value</management>		//Value=online or offline
<status>value</status>			//Value=closed, started or finished
<paper>
<pid></pid>
<status></status>	//Value is Y or N, represent the paper is finished or not; The initial status of paper is "N"
</paper>
<paper>
<pid></pid>
<status></status>
</paper>
<paper>
<pid></pid>
<status></status>
</paper>
</EINF>

400 NOPERMISSION[CL]
[CL]

404 NOTFOUND[CL]
[CL]

7.
MEINF eid[CL]
Cookie: value[CL]
[CL]
<?xml version="1.0"?>
<MEINF>
<name></name>
<course></course>
<stime>2012-05-03 10:00:00</stime>
<etime>2012-05-03 11:00:00</etime>
<accomplishment></accomplishment> 
<management></management>
<status></status>
</MEINF>

Return:
200 OK[CL]
[CL]

400 NOPERMISSION[CL]
[CL]

404 NOTFOUND[CL]
[CL]

