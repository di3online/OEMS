10.
ADDQ[CL]
Cookie: value[CL]
[CL]
<?xml version="1.0"?>
<ADDQ>
<description></description>
<choice></choice>		//The number of choices is either 2 or 4
<key></key>			//key=1, 2, 3, 4, corresponding the position of the right answer in XML
<time></time>
</ADDQ>

Return:
200 OK[CL]
[CL]

400 NOPERMISSION[CL]
[CL]

11.
DELQ qid[CL]
Cookie: value[CL]
[CL]

Return:
200 OK[CL]
[CL]

400 NOPERMISSION[CL]
[CL]

404 NOTFOUND[CL]
[CL]

12.
MQINF qid[CL]
Cookie: value[CL]
[CL]//BUF 为类似Tue May 15 14:46:02 2007格式的，p为时间结构体
<?xml version="1.0"?>
<MQINF>
<description></description>
<choice></choice>		//The number of choices is either 2 or 4
<key></key>
<time></time>
</MQINF>

Return:
200 OK[CL]
[CL]

400 NOPERMISSION[CL]
[CL]

404 NOTFOUND[CL]
[CL]

13.
LSTQ pid[CL]
Cookie: value[CL]
[CL]

Return:
200 OK[CL]
[CL]
<?xml version="1.0"?>
<LSTQ>
<question>		//The number of questions is equal to or above zero
<qid></qid>
<description></description>
<choice></choice>	//The number of choice is either 2 or 4
<key></key>		//key=1, 2, 3, 4, corresponding the position of the right answer in XML
<time></time>
</question>
</LSTQ>

400 NOPERMISSION[CL]
[CL]

14.
ERES eid[CL]
Cookie: value[CL]
[CL]

Return:
200 OK[CL]
[CL]
<?xml version="1.0"?>
<ERES>
<student>		//Number of students is equal to or above zero
<name></name>
<uid></uid>
<grade></grade>
<paper></paper>	//Value is 1, 2, or 3
</student>
</ERES>

15.
MPSTA pid[CL]		//Modify paper status to "Y"; The initial status of paper is "N"
Cookie: value[CL]
[CL]

Return:
200 OK[CL]
[CL]

400 NOPERMISSION[CL]
[CL]

404 NOTFOUND[CL]
[CL]

-----------------------------------------------------------------------------------
//Admin Module
-----------------------------------------------------------------------------------
16.
USRADD[CL]	//Add a user
Cookie: value[CL]
[CL]
<?xml version="1.0"?>
<USRADD>
<uid><uid>
<gid></gid>
<name></name>
<photo></photo>
</USRADD>
Return:
200 OK[CL]
[CL]

400 NO PERMISSION[CL]
[CL]

17.
USRDEL uid[CL]		//Delete a user
Cookie: value[CL]
[CL]

Return:
200 OK[CL]
[CL]

400 NOPERMISSION[CL]
[CL]

404 NOTFOUND[CL]
[CL]

